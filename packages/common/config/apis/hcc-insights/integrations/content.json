          },
          "required": [
            "name",
            "display_name"
          ],
          "type": "object"
        },
        "HttpType": {
          "enum": [
            "GET",
            "POST",
            "PUT"
          ],
          "type": "string"
        },
        "LocalDateTime": {
          "example": "2022-03-10T12:15:50",
          "format": "date-time",
          "type": "string"
        },
        "Meta": {
          "properties": {
