              "$ref": "#/components/schemas/Application"
            },
            "description": {
              "type": "string"
            },
            "display_name": {
              "type": "string"
            },
            "id": {
              "$ref": "#/components/schemas/UUID"
            },
            "name": {
              "maxLength": 255,
