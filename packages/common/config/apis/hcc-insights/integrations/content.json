              },
              "type": "array",
              "uniqueItems": true
            },
            "id": {
              "$ref": "#/components/schemas/UUID"
            },
            "name": {
              "maxLength": 255,
              "pattern": "[a-z][a-z_0-9-]*",
              "type": "string"
            }
          },
          "required": [
